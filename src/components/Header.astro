---
import logo from '../assets/AspireLogo.png'
---
<header class="header">
    <div class="container">
        <div class="header__logo">
            <img src={logo.src} alt="logo" class="header__logo-image" width={196} />
        </div>
    </div>
</header>

<style> 
.header {
    display: flex;
    justify-content: space-between;
    align-items: center;
}

.header__logo-image {
    padding: 16px 0;
}
</style>

